{
  "expo": {
    "name": "scantron",
    "slug": "scantron",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/scantron.png",
    "scheme": "scantron",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.infynnity.scantron",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/scantron.png",
        "backgroundColor": "#232634"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.scan",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/scantron.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/scantron.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#232634"
        }
      ],
      "expo-asset",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f5b38d20-09c4-4b90-9f90-77f73ac6d856"
      }
    },
    "owner": "fynb"
  }
}
